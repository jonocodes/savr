{
  "extends": "expo/tsconfig.base",
  "compilerOptions": {
    "strict": true,
    "baseUrl": ".",
    "paths": {
      "@/*": [
        "./*"
      ],
      "@savr/lib": ["../lib/src"],
      "@savr/lib/*": ["../lib/src/*"]
    },
    "moduleResolution": "bundler",
    "module": "Preserve",
    // "moduleResolution": "nodenext",
    // "allowImportingTsExtensions": true
  },
  "include": [
    "**/*.ts",
    "**/*.tsx",
    "../lib/src"
  ],
  "references": [{ "path": "../lib" }]
}


