  "name": "savr",
  "version": "0.2.0",
  "description": "Savr server, cli, and lib",
  "main": "./dist/cli.js",
  "types": "./dist/cli.d.ts",
  "type": "module",
  "bin": {
    "savr": "./dist/cli.js"
  },
  "exports": "./dist/cli.js",
  "scripts": {
    "test": "jest",
    "synclinks": "copyfiles -u 2 -F 'src/templates/**/*' 'dist/templates' && copyfiles -u 2 -F 'src/static/**/*' 'dist/static'",
    "build": "pkgroll && npm run synclinks",
    "ingestSample": "tsx ./src/cli.ts https://www.apalrd.net/posts/2023/network_ipv6/",
    "web": "node ./dist/cli.js --start-server",
    "web:dev": "tsx watch -r tsconfig-paths/register ./src/cli.ts --start-server",
    "ui": "tsx ./src/cli.ts --ui",
    "cli": "tsx ./src/cli.ts",
    "savr": "tsx ./src/cli.ts",
    "clean": "rm -rf node_modules dist"
  },
